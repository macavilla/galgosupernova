{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@constants/*": ["src/constants/*"],
			"@images/*": ["src/assets/images/*"],
			"@styles/*": ["src/styles/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	},
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"]
}
